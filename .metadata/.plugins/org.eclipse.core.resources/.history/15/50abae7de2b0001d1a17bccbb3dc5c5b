package com.examples;

import java.util.Scanner;

public class FibExample {

	public static void main(String[] args) {
		//0 1 1 2 3 5 8...
		  int n=10, a = 0, b = 0, c = 1;
	        System.out.print("Fibonacci Series:");
	        for(int i = 1; i <= n; i++)
	        {
	            a = b;
	            b = c;
	            c = a + b;
	            System.out.print(a+" ");
	        }

	}

}
